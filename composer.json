{
  "name"             : "bear-shen/tiny_framework",
  "description"      : "",
  "keywords"         : [
	"framework"
  ],
  "license"          : "MIT",
  "type"             : "project",
  "require"          : {
	"php"          : "^7.4",
	"predis/predis": "^1.1",
	"ext-gd"       : "*",
	"ext-openssl"  : "*",
	"ext-curl"     : "*",
	"ext-mbstring" : "*",
	"ext-json"     : "*",
	"ext-pdo"      : "*",
	"ext-fileinfo" : "*"
  },
  "require-dev"      : {
  },
  "autoload"         : {
	"classmap": [
	],
	"psr-4"   : {
	  "ControllerCli\\": "controller_cli/",
	  "Controller\\"   : "controller/",
	  "Lib\\"          : "lib/",
	  "Model\\"        : "model/",
	  "Middleware\\"   : "middleware/"
	},
	"files"   : [
	  "config.php",
	  "lib/bootloader.php",
	  "lib/helper.php"
	]
  },
  "autoload-dev"     : {
	"psr-4": {
	  "Tests\\": "tests/"
	}
  },
  "extra"            : {
	"laravel": {
	  "dont-discover": [
	  ]
	}
  },
  "scripts"          : {
  },
  "config"           : {
	"preferred-install"  : "dist",
	"sort-packages"      : true,
	"optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable"    : true,
  "repositories"     : {
	"packagist": {
	  "type": "composer",
	  "url" : "https://mirrors.aliyun.com/composer/"
	}
  }
}
